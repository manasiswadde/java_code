import java.util.*;
class First_Non_Repetive_Char{
    public static void main(String[] args) {
       String str1="hello";
       
        for(int i=0;i<str1.length();i++){
            char c=str1.charAt(i);
            if(str1.indexOf(c)==str1.lastIndexOf(c)){
            System.out.print(c);
            return;
        } 
        }
       
        System.out.print("no non repeating character");
    }
}

output: h
