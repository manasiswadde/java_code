class Main {
    public static void main(String[] args) {
        String str1="communication";
        String str2="com";
        String res="";
        for(int i=0;i<str1.length();i++){
           char ch=str1.charAt(i);
           if(str2.indexOf(ch)==-1){
               res=res+ch;
           }
        }
        System.out.print(res);
        
    }
}
output:
uniatin
