
import java.util.*;
class Main {
    public static void main(String[] args) {
        int arr[]={1,2,3,4,5};
        int k=2;
        k=k%arr.length;
        while(k>0){
            int temp=arr[arr.length-1];
            for(int i=arr.length-1;i>0;i--){
                arr[i]=arr[i-1];
                
            }
            arr[0]=temp;
            k--;
           
        }
         System.out.print(Arrays.toString(arr));
    }
}
output-[4, 5, 1, 2, 3]
